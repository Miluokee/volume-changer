{
  "name": "SRBK VolChanger",
  "version": "1.0.4",
  "description": "Build an Volume-Change Extension!",
  "default_locale": "en",
  "icons": {
    "16": "icons/icon-16.png",
    "19": "icons/icon-19.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon-16.png",
      "19": "icons/icon-19.png"
    },
    "default_popup": "popup.html",
    "default_title": "Volume Booster"
  },
  "permissions": [
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [ "./script/background.js" ],
    "persistent": false
  },
  "manifest_version": 2
}